{
  "kind": "collectionType",
  "collectionName": "product_categories",
  "info": {
    "singularName": "product-category",
    "pluralName": "product-categories",
    "displayName": "Product_Category",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "description": {
      "type": "text"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "product_categories"
    }
  }
}
